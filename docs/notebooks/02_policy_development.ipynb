   "source": [
    "## Context-Aware Masking Rules\n",
    "\n",
    "Context rules allow different masking behavior based on where PII appears in the document (headings, tables, footers, etc.)."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create a policy with context-aware rules\n",
    "context_aware_policy = \"\"\"\n",
    "version: \"1.0\"\n",
    "name: \"context-aware-healthcare\"\n",
    "description: \"Healthcare policy with context-specific rules\"\n",
    "\n",
    "locale: \"en\"\n",
    "seed: \"context-demo-seed\"\n",
    "\n",
    "# Default strategy\n",
    "default_strategy:\n",
    "  kind: \"redact\"\n",
    "  parameters:\n",
    "    redact_char: \"*\"\n",
    "    preserve_length: true\n",
    "\n",
    "# Entity-specific strategies\n",
    "per_entity:\n",
    "  PERSON:\n",
    "    kind: \"template\"\n",
    "    parameters:\n",
    "      template: \"[PATIENT]\"\n",
    "    threshold: 0.7\n",
    "    \n",
    "  EMAIL_ADDRESS:\n",
    "    kind: \"partial\"\n",
    "    parameters:\n",
    "      visible_chars: 3\n",
    "      position: \"start\"\n",
    "    threshold: 0.6\n",
    "    \n",
    "  US_SSN:\n",
    "    kind: \"template\"\n",
    "    parameters:\n",
    "      template: \"XXX-XX-XXXX\"\n",
    "    threshold: 0.9\n",
    "\n",
    "# Context-specific rules\n",
    "context_rules:\n",
    "  heading:\n",
    "    enabled: false  # Don't mask PII in headings to preserve readability\n",
    "    \n",
    "  table:\n",
    "    enabled: true\n",
    "    threshold_overrides:  # Higher thresholds in tables\n",
    "      PERSON: 0.8\n",
    "      US_SSN: 0.95\n",
    "      \n",
    "  footer:\n",
    "    enabled: false  # Don't mask contact information in footers\n",
    "    \n",
    "  body:\n",
    "    enabled: true  # Normal masking in body text\n",
    "\n",
    "# Allow list - these won't be masked even if detected as PII\n",
    "allow_list:\n",
    "  - \"Hospital Administration\"\n",
    "  - \"Emergency Contact\"\n",
    "  - \"admin@hospital.org\"  # Official contact emails\n",
    "  - \"customer.service@bank.com\"\n",
    "\n",
    "# Deny list - these will always be masked regardless of confidence\n",
    "deny_list:\n",
    "  - \"confidential\"\n",
    "  - \"CONFIDENTIAL\"\n",
    "  - \"internal use only\"\n",
    "  - \"classified\"\n",
    "\"\"\"\n",
    "\n",
    "# Save and load the policy\n",
    "policy_file = temp_dir / \"context_aware_policy.yaml\"\n",
    "with open(policy_file, 'w') as f:\n",
    "    f.write(context_aware_policy)\n",
    "\n",
    "print(f\"✅ Context-aware policy created: {policy_file}\")\n",
    "\n",
    "# Apply to healthcare document\n",
    "context_result = mask_document(\n",
    "    input_path=str(healthcare_file),\n",
    "    policy_path=str(policy_file),\n",
    "    output_format=\"docling\"\n",
    ")\n",
    "\n",
    "# Load and display results\n",
    "with open(context_result.masked_path, 'r') as f:\n",
    "    masked_healthcare = json.load(f)\n",
    "\n",
    "print(\"\\n📄 Context-aware masking results:\")\n",
    "print(\"\\nOriginal vs Masked:\")\n",
    "for orig, masked in zip(healthcare_doc[\"texts\"], masked_healthcare[\"texts\"]):\n",
    "    context = orig.get(\"context\", \"body\")\n",
    "    print(f\"\\n[{context.upper()}]\")\n",
    "    print(f\"  Original: {orig['text']}\")\n",
    "    print(f\"  Masked:   {masked['text']}\")\n",
    "    if orig['text'] == masked['text']:\n",
    "        print(f\"  ✅ No masking applied (context rule: {context})\")\n",
    "    else:\n",
    "        print(f\"  🎭 Masking applied\")"
   ]
